{
  "name": "@compromis/blobby",
  "version": "1.0.0",
  "description": "Sass framework for Compromís sites",
  "dependencies": {
    "vue": "^3.2.16",
    "bootstrap": "^5.1.3"
  },
  "devDependencies": {
    "node-sass": "^6.0.1",
    "@vitejs/plugin-vue": "^1.9.3",
    "vite": "^2.6.4"
  },
  "main": "index.js",
  "style": "css/blobby.css",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compromis/blobby.git"
  },
  "scripts": {
    "build-style": "node-sass --output-style compressed --source-map true ./scss/blobby.scss ./dist/css/blobby.css",
    "dev-style": "npm run build -- --watch",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview"
  },
  "keywords": [
    "compromis"
  ],
  "author": "Compromís",
  "license": "MIT"
}
